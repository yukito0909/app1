<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity7">

    <TextView
        android:id="@+id/labelTransport"
        android:layout_width="104dp"
        android:layout_height="34dp"
        android:layout_marginTop="16dp"
        android:text="優先"
        app:layout_constraintEnd_toStartOf="@id/transport"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <TextView
        android:id = "@+id/labelIpAddress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text = "IPアドレス"
        android:layout_marginTop="16dp"
        android:layout_marginStart="16dp"

        app:layout_constraintTop_toBottomOf="@id/labelTransport"
        app:layout_constraintStart_toStartOf="parent"/>
    <TextView
        android:id = "@+id/ipAddress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:text = "192.168.0.1"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@id/labelIpAddress"
        app:layout_constraintStart_toStartOf="@id/labelIpAddress"/>

    <TextView
        android:id="@+id/transport"
        android:layout_width="81dp"
        android:layout_height="34dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/labelTransport"
        app:layout_constraintTop_toTopOf="@+id/labelTransport"
        tools:text="Wi-Fi" />


</androidx.constraintlayout.widget.ConstraintLayout>